URxvt*font: -*-terminus-medium-*-*-*-14-*-*-*-*-*-*-*
URxvt*VT100.color4: CornflowerBlue
URxvt*saveLines: 1000
URxvt*scrollBar: false
URxvt*secondaryScroll: true
URxvt*tabbed.saveLines: 1000
URxvt*tabbed.scrollBar: false
URxvt*tabbed.tabbar-fg: 3
URxvt*tabbed.tabbar-bg: 0
URxvt*tabbed.tab-fg: 0
URxvt*tabbed.tab-bg: 1
URxvt*tabbed.secondaryScroll: true
URxvt*inheritPixmap: true 
URxvt*shading: 40
URxvt.transparent: true
URxvt*clipboard.autocopy: true
URxvt*perl-lib: /usr/lib/urxvt/perl/
URxvt*perl-ext-common: default,matcher,keyboard-select
URxvt*url-launcher: /usr/bin/xdg-open
URxvt*matcher.button: 1
URxvt*matcher.rend.0: Uline Bold fg5
URxvt*cursorBlink: 1
URxvt*keysym.M-Escape: perl:keyboard-select:activate



URxvt*foreground: white
URxvt*background: black

